	if replayMode and isPlaying and replayData then
		local now = tick()
		if now - lastUpdate >= (0.1 / playbackSpeed) then -- 10 FPS base rate
			currentFrame = currentFrame + 1
			if currentFrame > #replayData.frames then
				currentFrame = #replayData.frames
				isPlaying = false
			end
			updateReplayFrame()
			lastUpdate = now
		end
