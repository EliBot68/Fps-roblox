	elseif input.KeyCode == Enum.KeyCode.Right then
		if replayData then
			currentFrame = math.min(currentFrame + 1, #replayData.frames)
			updateReplayFrame()
		end
	elseif input.KeyCode == Enum.KeyCode.Left then
		currentFrame = math.max(currentFrame - 1, 1)
		updateReplayFrame()
	end
