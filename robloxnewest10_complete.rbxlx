		local relativeX = (mouse.X - progressBar.AbsolutePosition.X) / progressBar.AbsoluteSize.X
		relativeX = math.clamp(relativeX, 0, 1)
		currentFrame = math.floor(relativeX * #replayData.frames) + 1
		updateReplayFrame()
	end
