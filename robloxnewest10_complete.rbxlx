		rapidThreatIncrease = 5,      -- Alert if 5+ threats in 30 seconds
		systemPerformanceDrop = 0.8   -- Alert if performance drops below 80%
	}
}

-- System state
local systemState = {
	activeBans = {},
	suspiciousPlayers = {},
	threatHistory = {},
	performanceMetrics = {
		threatsDetected = 0,
		bansIssued = 0,
		kicksIssued = 0,
		falsePositives = 0,
		systemUptime = tick()
	},
	adminAlerts = {},
