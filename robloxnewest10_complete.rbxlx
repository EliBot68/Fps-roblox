		fadeTween.Completed:Connect(function()
			entry:Destroy()
		end)
	end)
	
	-- Keep only recent entries
	local children = killFeedFrame:GetChildren()
	if #children > 10 then -- Keep only last 10 entries
		for i = 1, #children - 10 do
