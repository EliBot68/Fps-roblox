		local parts = string.split(formatted, ".")
		local intPart = parts[1]
		local decPart = parts[2]
		
		-- Add commas
		local result = ""
		for i = 1, #intPart do
			if i > 1 and (i - 1) % 3 == 0 then
				result = "," .. result
			end
			result = string.sub(intPart, #intPart - i + 1, #intPart - i + 1) .. result
		end
		
		if decPart then
			result = result .. "." .. decPart
		end
		
