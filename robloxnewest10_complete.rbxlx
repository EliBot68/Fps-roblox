	Logging.Warn("GameStateManager", "Emergency reset triggered - Reason: " .. (reason or "Unknown"))
	
	-- Clear all state data
	stateData = {}
	
	-- Force return to lobby
	GameStateManager.ForceTransition(GameState.LOBBY, "emergency_reset")
	
