			return false
		end
		
		if not animationId:match("^rbxassetid://") then
			warn("[AnimationManager] Invalid animation ID format:", animationId)
			return false
		end
	end
	
	-- Add to registry
	ANIMATION_REGISTRY[weaponId] = animations
	print("[AnimationManager]  Added animations for weapon:", weaponId)
	
