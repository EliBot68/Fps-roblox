local lastUpdate = tick()
RunService.Heartbeat:Connect(function()
	local now = tick()
	
	-- Update every 5 seconds
	if now - lastUpdate >= 5 then
		updatePerformanceMetrics()
		collectGameMetrics()
		checkAlerts()
		storeSnapshot()
