function ArchitecturalCore.GetHealthMetrics(): {
	totalServices: number,
	coreServices: number,
	domainServices: number,
	infrastructureServices: number,
	dependencyViolations: number
}
	local coreCount = 0
	local domainCount = 0
	local infraCount = 0
	
	for serviceName, registration in pairs(serviceRegistry) do
