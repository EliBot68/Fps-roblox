	table.insert(cameraData.snapshots, {
		direction = lookDirection,
		timestamp = currentTime
	})
	
	-- Limit snapshot history
	if #cameraData.snapshots > MAX_SNAPSHOTS then
		table.remove(cameraData.snapshots, 1)
