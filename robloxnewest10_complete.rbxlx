	local RemoteRoot = ReplicatedStorage:WaitForChild("RemoteEvents")
	local CombatEvents = RemoteRoot:WaitForChild("CombatEvents")
	local fireWeaponRemote = CombatEvents:WaitForChild("FireWeapon")
	
