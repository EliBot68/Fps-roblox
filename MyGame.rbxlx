function countEnabledTasks(): number
	local count = 0
	for _, task in pairs(automationTasks) do
		if task.enabled then
			count = count + 1
		end
	end
	return count
end

-- Auto-initialize when server starts
AutomationOrchestrator.Initialize()

return AutomationOrchestrator
