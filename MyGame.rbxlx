-- Test Runner
local function runAllMatchmakingTests()
	print("ðŸŽ¯ Starting Matchmaking System Test Suite...")
	
	local results = {
		RatingSystemTestSuite:Run(),
		QueueManagerTestSuite:Run(),
		MatchmakingEngineTestSuite:Run(),
		IntegrationTestSuite:Run(),
