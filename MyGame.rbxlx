	weaponLabel.Text = "M4A1 Carbine"
	weaponLabel.TextColor3 = Color3.new(0.8, 0.8, 0.8)
	weaponLabel.TextScaled = true
	weaponLabel.Font = Enum.Font.SourceSans
	weaponLabel.TextStrokeTransparency = 0
