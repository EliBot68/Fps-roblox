-- Session data structure
local function createSessionData(player)
	return {
		userId = player.UserId,
		username = player.Name,
		joinTime = os.time(),
		serverId = SERVER_ID,
		position = nil,
		health = 100,
		currency = 0,
		inventory = {},
		matchState = {},
		preferences = {},
