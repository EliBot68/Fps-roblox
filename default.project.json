{
  "name": "Enterprise FPS Roblox - Production Ready",
  "servePort": 34872,
  "globIgnorePaths": [
    "**/*.rbxl",
    "**/*.rbxlx", 
    "**/*.rbxlx.lock",
    "**/.git",
    "**/.gitignore",
    "**/node_modules/**",
    "**/.vscode/**",
    "**/.idea/**",
    "**/dist/**",
    "**/build/**",
    "**/logs/**",
    "**/temp/**",
    "**/coverage/**",
    "**/.nyc_output/**",
    "**/docs/generated/**"
  ],
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "$path": "src/ReplicatedStorage",
      "RemoteEvents": {
        "$className": "Folder",
        "$path": "src/ReplicatedStorage/RemoteEvents"
      },
      "Shared": {
        "$className": "Folder", 
        "$path": "src/ReplicatedStorage/Shared"
      },
      "Assets": {
        "$className": "Folder",
        "$path": "assets"
      },
      "Localization": {
        "$className": "Folder",
        "$path": "src/ReplicatedStorage/Localization"
      }
    },

    "ReplicatedFirst": {
      "$className": "ReplicatedFirst",
      "$path": "src/ReplicatedFirst"
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$path": "src/ServerScriptService",
      "Core": {
        "$className": "Folder",
        "$path": "src/ServerScriptService/Core"
      },
      "Systems": {
        "$className": "Folder", 
        "$path": "src/ServerScriptService/Systems"
      },
      "Economy": {
        "$className": "Folder",
        "$path": "src/ServerScriptService/Economy"
      },
      "Analytics": {
        "$className": "Folder",
        "$path": "src/ServerScriptService/Analytics"
      },
      "Security": {
        "$className": "Folder",
        "$path": "src/ServerScriptService/Security"
      }
    },

    "ServerStorage": {
      "$className": "ServerStorage",
      "$path": "src/ServerStorage",
      "Data": {
        "$className": "Folder",
        "$path": "src/ServerStorage/Data"
      },
      "Templates": {
        "$className": "Folder",
        "$path": "src/ServerStorage/Templates"
      }
    },

    "StarterGui": {
      "$className": "StarterGui",
      "$path": "src/StarterGui",
      "$properties": {
        "ResetPlayerGuiOnSpawn": false,
        "ScreenOrientation": "LandscapeSensor"
      },
      "Interfaces": {
        "$className": "Folder",
        "$path": "src/StarterGui/Interfaces"
      },
      "Themes": {
        "$className": "Folder",
        "$path": "src/StarterGui/Themes"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "$path": "src/StarterPlayer",
      "$properties": {
        "AutoJumpEnabled": false,
        "CameraMaxZoomDistance": 25,
        "CameraMinZoomDistance": 0.5,
        "CameraMode": "Classic",
        "CharacterJumpHeight": 7.2,
        "CharacterMaxSlopeAngle": 89,
        "CharacterWalkSpeed": 16,
        "DevCameraOcclusionMode": "Zoom",
        "DevComputerCameraMovementMode": "UserChoice",
        "DevComputerMovementMode": "UserChoice",
        "DevTouchCameraMovementMode": "UserChoice",
        "DevTouchMovementMode": "UserChoice",
        "EnableMouseLockOption": true,
        "HealthDisplayDistance": 100,
        "LoadCharacterAppearance": true,
        "NameDisplayDistance": 100,
        "UserEmotesEnabled": true
      },
      "StarterCharacterScripts": {
        "$className": "StarterCharacterScripts",
        "$path": "src/StarterPlayer/StarterCharacterScripts"
      },
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts", 
        "$path": "src/StarterPlayer/StarterPlayerScripts",
        "Controllers": {
          "$className": "Folder",
          "$path": "src/StarterPlayer/StarterPlayerScripts/Controllers"
        },
        "UI": {
          "$className": "Folder",
          "$path": "src/StarterPlayer/StarterPlayerScripts/UI"
        }
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true,
        "StreamingEnabled": true,
        "StreamingTargetRadius": 512,
        "StreamingMinRadius": 64,
        "Gravity": 196.2,
        "FallenPartsDestroyHeight": -500,
        "AllowThirdPartySales": false,
        "SignalBehavior": "Immediate",
        "TouchesUseCollisionGroups": true
      },
      "Maps": {
        "$className": "Folder",
        "$path": "maps"
      },
      "Lighting": {
        "$className": "Folder",
        "$path": "lighting"
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Ambient": [0.1, 0.1, 0.15],
        "Brightness": 2.5,
        "EnvironmentDiffuseScale": 0.3,
        "EnvironmentSpecularScale": 0.3,
        "GlobalShadows": true,
        "Outlines": false,
        "ShadowSoftness": 0.2,
        "Technology": "ShadowMap",
        "TimeOfDay": "14:00:00",
        "FogEnd": 100000,
        "FogStart": 0,
        "GeographicLatitude": 41.733,
        "ColorShift_Bottom": [0, 0, 0],
        "ColorShift_Top": [0, 0, 0]
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "AmbientReverb": "NoReverb",
        "DistanceFactor": 3.33,
        "DopplerScale": 1,
        "RespectFilteringEnabled": true,
        "RolloffScale": 1,
        "VolumetricAudio": "Automatic"
      }
    },
    
    "Players": {
      "$className": "Players",
      "$properties": {
        "CharacterAutoLoads": true,
        "MaxPlayers": 100,
        "PreferredPlayers": 50,
        "RespawnTime": 3
      }
    },
    
    "Teams": {
      "$className": "Teams"
    },
    
    "Chat": {
      "$className": "Chat",
      "$properties": {
        "LoadDefaultChat": true
      }
    },
    
    "TestService": {
      "$className": "TestService",
      "$properties": {
        "AutoRuns": false,
        "IsPhysicsEnvironmentalThrottled": true,
        "IsSleepAllowed": true,
        "NumberOfPlayers": 0,
        "SimulateSecondsLag": 0,
        "Timeout": 10
      }
    }
  }
}